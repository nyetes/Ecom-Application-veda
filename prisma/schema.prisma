generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id          Int      @id @default(autoincrement())
  title       String
  description String
  price       Float
  category    String
  image       String
  rating      Float
}

model Category {
  id       Int      @id @default(autoincrement())
  name     String
  products Product[]
}
